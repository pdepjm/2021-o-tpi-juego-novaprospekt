import wollok.game.*
import texto.*
import managerDeNiveles.*

program juego {
	//Titulo del juego
	game.title("Plumber")
	
	//Configuro el tamaño de los sprites (Aca son de 32x32)
	game.cellSize(32)
	
	//Tamaño del tablero de juego  
	game.height(10)
	game.width(10)
	
	//Inicializa los niveles y carga el Nivel 1 desde el manager de niveles
	managerDeNiveles.configurarNiveles()
	
	game.boardGround("imagen_fondo.png")
	
	//Inicia el juego
	game.start()
	
}